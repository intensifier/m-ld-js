{
  "name": "@m-ld/m-ld",
  "version": "0.8.2",
  "description": "m-ld native Javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./dist/orm": "./dist/orm/index.js",
    "./dist/mqtt": "./dist/mqtt/index.js",
    "./dist/socket.io": "./dist/socket.io/index.js",
    "./dist/socket.io-server": "./dist/socket.io/server/index.js",
    "./dist/ably": "./dist/ably/index.js",
    "./dist/wrtc": "./dist/wrtc/index.js",
    "./dist/memdown": "./dist/memdown/index.js",
    "./dist/security": "./dist/security/index.js",
    "./dist/shacl": "./dist/shacl/index.js",
    "./dist/constraints/WritePermitted": "./dist/constraints/WritePermitted.js",
    "./dist/constraints/Statutory": "./dist/constraints/Statutory.js"
  },
  "scripts": {
    "link-locals": "npm link ../m-ld-spec ../json-rql ../typedoc-theme",
    "clean": "rm -rf dist _site",
    "build": "npm run clean && jest --silent --runInBand && tsc && typedoc",
    "dev": "concurrently -r -k \"jest --watch --silent --runInBand\" \"tsc --watch\"",
    "dev+log": "jest --watch --runInBand",
    "compliance": "tsc --incremental && node compliance/test.js",
    "prepublishOnly": "npm run build && npm version $VERSION && git push",
    "postpublish": "npm dist-tag add \"@m-ld/m-ld@\"${npm_package_version} $(git branch --no-color --show-current)",
    "doc-dev": "typedoc && concurrently \"onchange -d 1000 'src/**/*.ts' 'doc/**/*.*' 'typedoc.js' -- typedoc\" \"browser-sync start --server _site --files _site\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m-ld/m-ld-js.git"
  },
  "author": "gsvarovsky@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/m-ld/m-ld-js/issues"
  },
  "homepage": "https://js.m-ld.org/",
  "devDependencies": {
    "@m-ld/typedoc-theme": "^0.1.4",
    "@peculiar/webcrypto": "^1.2.3",
    "@types/jest": "^26.0.20",
    "@types/memdown": "^3.0.0",
    "@types/n3": "^1.4.4",
    "@types/readable-stream": "^2.3.9",
    "@types/ws": "^7.4.6",
    "ably": "^1.2.10",
    "aedes": "^0.46.0",
    "async-mqtt": "^2.6.1",
    "browser-sync": "^2.26.14",
    "concurrently": "^6.0.1",
    "generic-pool": "^3.8.2",
    "jest": "^27.0.6",
    "jest-mock-extended": "^1.0.11",
    "leveldown": "^6.0.0",
    "memdown": "^6.0.0",
    "n3": "^1.8.0",
    "onchange": "^7.1.0",
    "restify": "^8.5.1",
    "simple-peer": "^9.11.0",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2",
    "tape": "^5.3.1",
    "tmp": "^0.2.1",
    "ts-jest": "^27.0.3",
    "typedoc": "^0.19.2",
    "typedoc-plugin-no-inherit": "1.2.2",
    "typescript": "^4.1.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/test/"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    }
  },
  "dependencies": {
    "@ably/msgpack-js": "^0.4.0",
    "@m-ld/m-ld-spec": "^0.6.7-0",
    "@types/abstract-leveldown": "^5.0.1",
    "@types/detect-node": "^2.0.0",
    "@types/jsonld": "^1.5.2",
    "@types/rdf-js": "^4.0.1",
    "@types/requestidlecallback": "^0.3.1",
    "@types/simple-peer": "^9.11.4",
    "@types/uuid": "^8.3.1",
    "@types/valid-data-url": "^2.0.0",
    "asynciterator": "^3.2.0",
    "json-rql": "^0.6.2",
    "jsonld": "^3.3.0",
    "loglevel": "^1.7.1",
    "marky": "^1.2.1",
    "mqtt-pattern": "^1.2.0",
    "quadstore": "^9.1.0",
    "quadstore-comunica": "^1.1.0",
    "rdf-data-factory": "^1.0.4",
    "rx-flowable": "^0.1.0",
    "rxjs": "^7.2.0",
    "short-uuid": "^4.1.0",
    "uuid": "^8.3.2",
    "valid-data-url": "^4.0.0"
  },
  "peerDependencies": {
    "@peculiar/webcrypto": "1",
    "ably": "1",
    "async-mqtt": "2",
    "memdown": "6",
    "simple-peer": "9",
    "socket.io": "4",
    "socket.io-client": "4"
  },
  "peerDependenciesMeta": {
    "socket.io": {
      "optional": true
    },
    "socket.io-client": {
      "optional": true
    },
    "async-mqtt": {
      "optional": true
    },
    "ably": {
      "optional": true
    },
    "simple-peer": {
      "optional": true
    },
    "memdown": {
      "optional": true
    },
    "@peculiar/webcrypto": {
      "optional": true
    }
  }
}
